<?xml version="1.0" encoding="utf-8"?>
<flectra>
    <data noupdate="1">

        <!-- Sequences for hosptal.patient -->
        <record id="seq_patient_hospital" model="ir.sequence">
            <field name="name">Patient Sequence</field>
            <field name="code">hospital.patient.sequence</field>
            <field name="prefix">HP</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences for hosptal.appointment -->
        <record id="seq_patient_appointment" model="ir.sequence">
            <field name="name">Appointment Sequence</field>
            <field name="code">hospital.appointment.sequence</field>
            <field name="prefix">HA</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</flectra>
